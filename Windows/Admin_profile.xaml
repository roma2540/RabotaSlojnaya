<Window x:Class="Arasaka_Employers.Windows.Admin_profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Arasaka_Employers.Windows"
        mc:Ignorable="d"
        Title="Admin_profile" Height="450" Width="400"
        ResizeMode="NoResize">
    <Grid Background="#FF4C4C4C">
        <Grid.RowDefinitions>
            <RowDefinition Height="379*"/>
            <RowDefinition Height="55*"/>
        </Grid.RowDefinitions>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="15" MinHeight="35" MinWidth="300" Content="Товар" Click="Button_Click"></Button>
            <Button Margin="15" MinHeight="35" MinWidth="300" Content="Сотрудники" Click="Button_Click_1"></Button>
            <Button Margin="15" MinHeight="35" MinWidth="300" Content="История" Click="Button_Click_2"></Button>
        </StackPanel>
        <Button Grid.Row="1" Margin="15" HorizontalAlignment="Left" Content="Выйти" MinWidth="100"></Button>
    </Grid>
</Window>
