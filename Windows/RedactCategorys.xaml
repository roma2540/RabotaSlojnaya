<Window x:Class="Arasaka_Employers.Windows.RedactCategorys"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Arasaka_Employers.Windows"
        mc:Ignorable="d"
        Title="RedactCategorys" Height="500" Width="400"
        ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="409*"/>
            <RowDefinition Height="75*"/>
        </Grid.RowDefinitions>
        <StackPanel VerticalAlignment="Center">
            <ComboBox Height="50" Margin="5"></ComboBox>
            <TextBox Height="50" Margin="5"></TextBox>
        </StackPanel>
        <Button Width="75" Margin="5" Content="Выйти" Grid.Row="1" HorizontalAlignment="Left"></Button>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Width="75" Margin="5" Content="Редактировать"></Button>
            <Button Width="75" Margin="5" Content="Создать"></Button>
        </StackPanel>
    </Grid>
</Window>
